<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jn.zfl.mySpringBoot.dao.MainDao">
<select id="getUserById" parameterType="Integer" resultType="com.jn.zfl.mySpringBoot.bean.User">
	select * from xxcl_user where id = #{user_id}
</select>

<select id="queryUserCount" parameterType="String" resultType="int">
	select count(0) from xxcl_user
</select>
<select id="querySome" parameterType="int" resultType="com.jn.zfl.mySpringBoot.bean.User">
	select * from xxcl_user limit #{start},#{end}
</select>
</mapper>